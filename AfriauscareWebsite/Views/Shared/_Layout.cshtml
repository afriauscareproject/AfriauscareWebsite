<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />

    <link rel="stylesheet" href="~/Content/Site.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" integrity="sha512-tS3S5qG0BlhnQROyJXvNjeEM4UpMXHrQfTGmbQ1gKmelCxlSEBUaxhRBj/EFTzpbP4RVSrpEikbmdJobCvhE3g==" crossorigin="anonymous" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css" integrity="sha512-sMXtMNL1zRzolHYKEujM2AqCLUR9F2C4/05cdbxjjLSRvMQIciEPCQZo++nk7go3BtSuK9kfa/s+a4f4i5pLkw==" crossorigin="anonymous" />

    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
    <script src="~/Scripts/popper.min.js" type="module"></script>

    <script src="~/Scripts/jquery-3.7.0.js"></script>
    <script src="~/Scripts/jquery-3.7.0.min.js"></script>
    <script src="~/Scripts/jquery-3.7.0.slim.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js" integrity="sha512-bPs7Ae6pVvhOSiIcyUClR7/q2OAsRiovw4vAkX+zJbw3ShAeeqezq50RIIcIURq7Oa20rW2n2q+fyXBNcU9lrw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
</head>

<body id="header">
    <div>
        <nav class="navbar navbar-expand-md fixed-top">
            <div class="container-fluid">
                <div class="logo">
                    <h1>
                        @Html.ActionLink("AFRI-AUS CARE", "Index", "Home", null)
                    </h1>
                </div>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#collapsibleNavbar">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse justify-content-end" id="collapsibleNavbar">
                    <ul class="navbar-nav d-flex px-3">
                        <li class="nav-item active px-2">
                            @Html.ActionLink("Home", "Index", "Home", null, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item dropdown px-2">
                            <a class="nav-link dropdown-toggle" href="@Url.Action("ProgramServices", "Home")">
                                <span>Programs&Services</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li> @Html.ActionLink("Events", "Events", "Home", null, new { @class = "nav-link" }) </li>
                            </ul>
                        </li>
                        <li class="nav-item px-2">
                            @Html.ActionLink("Gallery", "Gallery2", "Home", null, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item dropdown px-2">
                            <a class="nav-link dropdown-toggle" href="@Url.Action("About", "Home")">
                                <span>About Us</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li> @Html.ActionLink("Board Members", "BoardMembers", "Home", null, new { @class = "nav-link" }) </li>
                                <li>@Html.ActionLink("Team", "Team", "Home", null, new { @class = "nav-link" })</li>
                                <li>@Html.ActionLink("Key Partners", "KeyPartners", "Home", null, new { @class = "nav-link" })</li>
                            </ul>
                        </li>
                        <li class="nav-item px-2">
                            @Html.ActionLink("Contact Us", "Contact", "Home", null, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item px-2">
                            @Html.ActionLink("Donate", "Donate", "Home", null, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item"> @Html.ActionLink("Login", "Login", "Session", null, new { @class = "nav-link" })</li>
                    </ul>
                </div>
            </div>
        </nav>
    </div>

    <div class="body-content">
        @RenderBody()
        <section id="clients" class="clients" style="background-color: rgba(0,0,0,0.03) !important;">
            <div class="container">
                <br />
                <div class="section-title" style="padding-bottom:0px;text-align:center">
                    <h2>STAY IN TOUCH</h2>
                </div>

                <div class="row">
                    <div class="col-lg-5 col-md-4"></div>
                    <div class="col-lg-3 col-md-4">
                        <a href="" class="btn-get-started animate__animated animate__fadeInUp">ENQUIRE NOW</a>
                    </div>
                    <div class="col-lg-3 col-md-4"></div>
                </div>
                <div class="row">
                    <div class="col-lg-5 col-md-4"></div>
                    <div class="col-lg-3 col-md-4">
                        <button type="button" class="btn btn-social-icon btn-facebook btn-rounded"><i class="bi bi-facebook"></i></button>
                        <button type="button" class="btn btn-social-icon btn-youtube btn-rounded"><i class="bi bi-youtube"></i></button>
                        <button type="button" class="btn btn-social-icon btn-linkedin btn-rounded"><i class="bi bi-linkedin"></i></button>
                        <button type="button" class="btn btn-social-icon btn-instagram btn-rounded"><i class="bi bi-instagram"></i></button>
                    </div>
                    <div class="col-lg-3 col-md-4"></div>
                </div>
                <div class="row">
                    <div class="col-lg-5 col-md-4"></div>
                    <div class="col-lg-3 col-md-4 et_pb_text_12">
                        <h3>OPENING HOURS</h3>
                        <p style="margin-left:20px;">Monday-Friday: 9am – 5pm</p>
                        <p style="margin-left: 20px;">Saturday: 10am – 3pm</p>
                        <p style="margin-left: 20px;">Sunday: 10am – 2pm</p>
                    </div>
                    <div class="col-lg-3 col-md-4"></div>
                </div>

            </div>
        </section>
        <!-- ======= Footer ======= -->
        <footer id="footer">

            <div class="footer-top">
                <div class="container">
                    <div class="row">

                        <div class="col-lg-2 col-md-6 footer-links">
                            <h4>Quick Links</h4>
                            <ul>
                                <li><i class="bi bi-chevron-right"></i> <a href="#">Home</a></li>
                                <li><i class="bi bi-chevron-right"></i> <a href="#">About us</a></li>
                                <li><i class="bi bi-chevron-right"></i> <a href="#">Programs & Services</a></li>
                                <li><i class="bi bi-chevron-right"></i> <a href="#">NDIS</a></li>
                                <li><i class="bi bi-chevron-right"></i> <a href="#">Contact Us</a></li>
                            </ul>
                        </div>

                        <div class="col-lg-2 col-md-6 footer-links">
                            <h4>Regulations</h4>
                            <ul>
                                <li><i class="bi bi-chevron-right"></i> <a href="#">Terms & Conditions</a></li>
                                <li><i class="bi bi-chevron-right"></i> <a href="#">Privacy Policy</a></li>
                                <li><i class="bi bi-chevron-right"></i> <a href="#">Careers</a></li>
                            </ul>
                        </div>
                        <div class="col-lg-3 col-md-6 footer-links">
                            <h4>Regulations</h4>
                            <ul>
                                <li> <a href="#">AFRI AUS CARE</a></li>
                                <li><a href="#">ACKNOWLEDGE THE</a></li>
                                <li><a href="#">TRADITIONAL OWNERS OF</a></li>
                                <li><a href="#">THE LAND</a></li>
                                <li><img loading="lazy" width="66" height="44" src="~/Content/Images/ack2.png" alt="" title="ack2" class="wp-image-13835"></li>
                                <li><img loading="lazy" width="66" height="44" src="~/Content/Images/ack.png" alt="" title="ack" class="wp-image-13836"></li>
                            </ul>
                        </div>
                        <div class="col-lg-3 col-md-6 footer-links">
                            <h4>Regulations</h4>
                            <ul>
                                <li><img loading="lazy" width="119" height="110" src="~/Content/Images/AAC_Logo_white.png" alt="" title="AAC_Logo_white" class="wp-image-13562"> </li>
                                <li><img loading="lazy" width="119" height="110" src="~/Content/Images/registered-charity.jpg" alt="" title="registered charity" srcset="https://afri-auscare.org/wp-content/uploads/2018/11/registered-charity.jpg 250w, https://afri-auscare.org/wp-content/uploads/2018/11/registered-charity-150x150.jpg 150w" sizes="(max-width: 250px) 100vw, 250px" class="wp-image-13037"></li>
                            </ul>
                        </div>
                        <div class="col-lg-2 col-md-6 footer-contact">
                            <h4>Contact Us</h4>
                            <p>
                                @TempData["Address"] <br>
                                @TempData["Suburb"]<br>
                                @TempData["State"] , @TempData["Postcode"]<br><br>
                                <strong>Phone:</strong> @TempData["Phone"]<br>
                                <strong>Email:</strong> @TempData["Email"]<br>
                            </p>
                        </div>

                    </div>
                </div>
            </div>

            <div class="container">
                <div class="copyright">
                    &copy; Copyright <strong><span>AFRI-AUS CARE 2023</span></strong>. All Rights Reserved
                </div>
            </div>
        </footer><!-- End Footer -->
    </div>

    <script>
        $(function () {
            $(document).scroll(function () {
                var $nav = $(".navbar-fixed-top");
                $nav.toggleClass('scrolled', $(this).scrollTop() > $nav.height());
            });
        });
    </script>

    @RenderSection("scripts", required: false)
</body>
</html>
