@model IEnumerable<Afriauscare.BusinessLayer.Shared.LogUserActivityModel>

@{
    Layout = null;
}


<title>@ViewBag.Title Home Admin Portal</title>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

@Html.Partial("_MenuBarAdminPortal")
<div class="container">
    <br />
    <h2>Last Changes Registered</h2>
    <br />
    <br />
    <table class="table table-striped table-hover">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Username</th>
                <th scope="col">Action</th>
                <th scope="col">Module</th>
                <th scope="col">Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.User_Full_Name)</td>
                    <td>@Html.DisplayFor(modelItem => item.Action_Excuted)</td>
                    <td>@Html.DisplayFor(modelItem => item.Module_Name)</td>
                    <td>@Html.DisplayFor(modelItem => item.Datetime_action)</td>
                </tr>
            }
        </tbody>
    </table>
</div>
